    "db:apply-sql:api": "tsx scripts/apply-sql-via-api.ts",
    "db:apply-migrations": "tsx scripts/apply-migrations.ts",
    "perf:lighthouse": "node scripts/lighthouse.mjs http://127.0.0.1:3000 /welcome /practice /mock /review",
